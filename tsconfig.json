{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    // "strictNullChecks": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@layouts/*": [
        "./layouts/*"
      ],
      "@icons/*": [
        "./icons/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@contexts/*": [
        "./contexts"
      ],
      "@styles/*": [
        "./styles/*"
      ],
    },
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/testswipe/index.js", "components/testswipe/index.js"],
  "exclude": ["node_modules"]
}
